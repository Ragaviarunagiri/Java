public class twodmin {
    public static void main(String args[]){
        int[][]arr={
            {10,20},
            {15,25}
        };
    int min =arr[0][0];
    for(int i=0;i<arr.length;i++){
        for(int j=0;j<arr[i].length;j++){
            if(arr[i][j]<min){
                min=arr[i][j];
            }
    }
    }
System.out.println("the minimum  element is"+min);
    }
}
